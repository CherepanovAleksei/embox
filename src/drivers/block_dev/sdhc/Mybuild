package embox.driver.sd

module sdhc {
	option number base_addr

	option number log_level=0

	source "sdhc.c"

	depends embox.driver.block_common
}

module imx6_sdhc {
	option number base_addr

	option number log_level=0

	source "imx6_sdhc.c"

	depends embox.driver.block_common
}

module dw_mmc {
	option number base_addr
	
	option number log_level=0
	
	source "dw_mmc.c"
	
	depends embox.driver.block_common
	depends embox.driver.clk.socfpga
}
